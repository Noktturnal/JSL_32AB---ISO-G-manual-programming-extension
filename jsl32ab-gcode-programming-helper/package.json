{
  "name": "jsl32ab-gcode-programming-helper",
  "displayName": "JSL32AB-GCODE_PROGRAMMING_HELPER",
  "description": "Custom language support for Fanuc JSL32AB G-code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jsl32ab-g.p.h",
        "aliases": ["JSL32AB-G.P.H.", "jsl32ab-g.p.h"],
        "extensions": [".nc", ".gcode", ".cnc", ".txt"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jsl32ab-g.p.h",
        "scopeName": "source.jsl32ab",
        "path": "./syntaxes/jsl32AB-g.p.h.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "JSL32AB Custom",
        "uiTheme": "vs-dark",
        "path": "./themes/jsl32ab-color-theme.json"
      }
    ]
  }
}
